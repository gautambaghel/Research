@article{Yang2014,
abstract = {Multiplayer Online Battle Arena (MOBA) games rely primarily$\backslash$non combat to determine the ultimate outcome of the$\backslash$ngame. Combat in these types of games is highly-dynamic$\backslash$nand can be difficult for novice players to learn. Typically,$\backslash$nmastery of combat requires that players obtain expert knowledge$\backslash$nthrough practice, which can be difficult to concisely$\backslash$ndescribe. In this paper, we present a data-driven approach$\backslash$nfor discovering patterns in combat tactics that are common$\backslash$namong winning teams in MOBA games. We model combat$\backslash$nas a sequence of graphs and extract patterns that predict$\backslash$nsuccessful outcomes not just of combat, but of the entire$\backslash$ngame. To identify those patterns, we attribute features to$\backslash$nthese graphs using well known graph metrics. These features$\backslash$nallow us to describe, in meaningful terms, how different combat$\backslash$ntactics contribute to team success. We also present an$\backslash$nevaluation of our methodology on the popular MOBA game,$\backslash$nDotA 2 (Defense of the Ancients 2). Experiments show that$\backslash$nextracted patterns achieve an 80{\%} prediction accuracy when$\backslash$ntesting on new game logs.},
author = {Yang, Pu and Harrison, Brent and Roberts, David L.},
file = {:D$\backslash$:/FDG March Paper/Related works/Chen/Yang2014-MOBASucessPatterns[moba].pdf:pdf},
journal = {Proceedings of Foundations of Digital Games 2014},
