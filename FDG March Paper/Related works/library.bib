journal = {Source: MIS Quarterly},
keywords = {Virtual team,repeated measures,temporality,transactive memory,trust},
number = {4},
pages = {783--808},
title = {{The Impact of Knowledge Coordination on Virtual Team Performance over Time The Impact of Knowledge Coordination on Virtual Team Performance over Time1}},
url = {http://www.jstor.org/stable/25148820{\%}5Cnhttp://about.jstor.org/terms},
